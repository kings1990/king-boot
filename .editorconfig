root = true

[*.{groovy, java, kt, kts, xml, xsd}]
charset = utf-8

indent_style = tab

indent_size = 4

end_of_line = lf

trim_trailing_whitespace = true

insert_final_newline = false


[*.{js, html}]
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = false
trim_trailing_whitespace = false